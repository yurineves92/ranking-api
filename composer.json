{
    "name": "yurineves92/ranking-api",
    "description": "API REST para obter ranking de movimentos usando Slim 4 e Eloquent ORM",
    "type": "project",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4",
        "illuminate/database": "^10.48",
        "slim/psr7": "^1.7",
        "nyholm/psr7": "^1.8",
        "zircote/swagger-php": "^4.9",
        "php-di/php-di": "^7.0",
        "doctrine/annotations": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
